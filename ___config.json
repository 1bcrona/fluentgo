{
    "serviceMode": "in",
    "log": {
        "enabled": true,
        "console": true,
        "type": "rolling",
        "path": "logs"
    },
    "inputs": {
        "maxMessageSize": -1,
        "queue": {
            "maxSize": 10240000,
            "maxCount": 2000,
            "onSeverySec": 30
        },
        "buffer": {
            "path": "buffer",
            "prefix": "bf",
            "extension": "buf",
            "timestampKey":"@timestamp",
            "timestampFormat":"2006-01-02T15:04:05.999-07:00",
            "flush": {
                "count": 1000,
                "size": 1024000
            }
        },
        "producers": [
            {
                "type": "tcp",
                "params":[
                    { "name": "enabled", "value": true },
                    { "name": "host", "value": "localhost:7889" },
                    { "name": "compressed", "value": false }
                ]
            }
        ]
    }
}